#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int findUniqueNumber(int arr[], int size) {
    int uniqueNumber = 0;

    for (int i = 0; i < size; i++)
        {
        uniqueNumber ^= arr[i];
    }

    return uniqueNumber;
}









int main()
{
  int a[] = {4,2,4};

  printf("\n%d",findUniqueNumber(a,3));
}
