#include <stdio.h>
#include <stdlib.h>
#include <string.h>

void myfunc(int a[],b[], int n)
{
    int i , j ;
    for (i = 0, j = n-1 ; i < n ; i++,j--)

    {
        b[i] = a[j];
    }
    for (int k = 0 ; k < n ; k ++)
    {
        printf("%d",b[k]);
    }

}

int main()
{
    
    int a[] = {1,2,3,4};
  myfunc(a,4);
}
