#include <stdio.h>
#include <stdlib.h>

int main(void) {

	float a,b,temp;

	printf("Enter value of a: ");
	fflush(stdout);
	scanf("%f",&a);

	printf("\nEnter value of b: ");
	fflush(stdout);
	scanf("%f",&b);

	temp=a;
	a=b;
	b=temp;

	printf("\nAfter swapping, value of a = %.2f",a);
	printf("\nAfter swapping, value of b = %.2f",b);

	return EXIT_SUCCESS;
}
