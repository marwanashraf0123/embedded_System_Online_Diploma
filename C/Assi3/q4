#include <stdio.h>
int main(void) {
	setbuf(stdout,NULL); //TO SOLVE INPUT/OUTPUT BUFFER BUG
	int n,i,arr[100],element,l;
	printf("enter the number of elements :\n ");
	scanf("%d",&n);
	for(i=0;i<n;i++){
		arr[i]=i+1;
	}
	for(i=0;i<n;i++){
		printf("%d   ",arr[i]);
	}
	printf("\nenter the element to be inserted :");
	scanf("%d",&element);
	printf("enter the location :");
	scanf("%d",&l);
	arr[l-1]=element;
	for(i=0;i<n;i++){
			printf("%d  ",arr[i]);
		}
}
